{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["ESNext"],
    "module": "CommonJS",
    "noEmit": true,
    "paths": {
      "react-native-story-view": ["../src"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": ["src"]
}
